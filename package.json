{
	"name": "artist-s_corner_be",
	"version": "1.0.0",
	"description": "Artist's corner Serverside",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev --require dotenv/config src/index.ts",
		"build": "tsc",
		"start": "node --require dotenv/config dist/index.js",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
		"migrate:down": "npx sequelize-cli db:migrate:undo:all",
		"migrate:undo": "npx sequelize-cli db:migrate:undo",
		"seed": "npx sequelize-cli db:seed:all",
		"seed:undo": "npx sequelize-cli db:seed:undo:all"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/express": "^4.17.21",
		"@types/node": "^22.5.0",
		"bcrypt": "^5.1.1",
		"cloudinary": "^2.5.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^5.0.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"multer": "^1.4.5-lts.1",
		"pg": "^8.12.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.37.3",
		"typescript": "^5.5.4",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/multer": "^1.4.12",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"nodemon": "^3.1.4",
		"sequelize-cli": "^6.6.2",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"ts-node-dev": "^2.0.0"
	}
}
